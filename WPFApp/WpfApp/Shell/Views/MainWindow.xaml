<Window x:Class="Shell.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Shell.Views"
        xmlns:prism ="http://prismlibrary.com/"
        xmlns:share ="clr-namespace:Skmioo.Application.Share.Prism;assembly=Skmioo.Application.Share"
        prism:ViewModelLocator.AutoWireViewModel="True"
        mc:Ignorable="d"
        Title="{Binding Title}" Height="450" Width="800">
    <ContentControl prism:RegionManager.RegionName="{x:Static share:RegionNames.MainRegion}"/>
</Window>
